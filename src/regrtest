#!/bin/bash
#not actually a regression test
OCAML=./ocaml.run
TMPDIR=/tmp
FLAGS=-dqexprs
POSFILE=postests
NEGFILE=negtests
RESFILE=$1

rm -f $TMPDIR/*.lilog

echo RUNNING POSFILE
for i in `cat $POSFILE`
do
  echo RUNNING: $i 
  $OCAML $FLAGS < $i &> $TMPDIR"/"${i/\//_}".lilog"
  if [ $? = 0 ] 
  then 
    echo "SUCCESS!"
  else
    echo "FAILURE :("
  fi
done
echo DONE

echo RUNNING NEGFILE
for i in `cat $POSFILE`
do
  echo RUNNING: $i 
  $OCAML $FLAGS < $i &> $TMPDIR"/"${i/\//_}".lilog"
  if [ $? = 0 ] 
  then 
    echo "SUCCESS!"
  else
    echo "FAILURE :("
  fi
done
echo DONE

FILE=`cat $POSFILE $NEGFILE`
FILE=${FILE/\//_}

#grovel through output files for stats below:

