qualif A(v) : sz l = v
qualif AA(v) : sz v = ~A 
qualif B(v) : sz v = 1 + sz ~A
qualif C(v) : sz v = sz ~A - 1

qualif D(v) : sz v = sz e0
qualif E(v) : sz v = sz e1

qualif ZEROC(v) : v { * * } 0
qualif N(v) : v { * * } ~A 
qualif ZERO(v) : sz v { * * } 0
qualif NZERO(v) : nil v { * * } [0, 1]

qualif EE(v) : v { * * } e0

qualif NIL(v) : v = nil ~A 
qualif NIL0(v) : nil v = 0
qualif NIL1(v) : nil v = 1

qualif FALSE(v) : (1 = 0)

