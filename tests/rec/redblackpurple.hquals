(* Color quals *)
qualif FALSE(v):          0 = 1
qualif COLOR(v):          color v = [0, 1, 2, 3, 4]
qualif COLORNOTPURPLE(v): color v <= [0, 1, 2]
qualif WHENPURPLE(v):     (color v <= 2) or (color ~A = 2)

(* Balance quals *)

qualif INC(v): height v = 1 + height ~A
qualif EQH(v): height v = height ~A
qualif BAL(v): height v = height e1

(* BST quals *)

qualif FALSE(v) : 1 = 0
qualif TCOMP(v) : v { >, < } e0
qualif COMP(v)  : ~A { >, < } v
