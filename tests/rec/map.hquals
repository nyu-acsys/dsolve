qualif FALSE(v) : 1 = 0
qualif TCOMP(v) : e0 { * * } v
qualif COMP(v)  : ~A { * * } v
qualif INTLIT(v): ^ { * * } v

qualif HH(v)    : v = ~A
qualif H(v)     : v { * * } height ~A
qualif BAL(v)   : height v { * * } height e2 + [1, 2, 3]
qualif BALA(v)  : height v { * * } height ~A + [1, 2, 3]
qualif HMEAS(v) : v { * * } height [e2, e3] + [1, 2, 3]


qualif Height(v): v = height t
qualif Pheight(v): v = pheight t
