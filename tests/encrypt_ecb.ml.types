type key =
    { data : garbage array;
      p_lsb : garbage array;
      p_msb : garbage array;
      (* subkeys: 18 elements of 16 bits *)
      p_lsb_rev : garbage array;
      p_msb_rev : garbage array;
      (* subkeys in reverse order *)
      s1_lsb : garbage array;
      s1_msb : garbage array;
      s2_lsb : garbage array;
      s2_msb : garbage array;
      s3_lsb : garbage array; 
      s3_msb : garbage array;
      s4_lsb : garbage array;
      s4_msb : garbage array;
      (* the four s-boxes: sNP, where N=0,1,2,3 denotes the box,
       * and P=0,1 whether the LSB (0) or MSB (1) of the box
       * value is stored.
       *)
    };;
