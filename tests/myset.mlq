val inter: x: 'a set -> y: 'a set -> {v: 'a set | v = inter x y}
val union: x: 'a set -> y: 'a set -> {v: 'a set | v = union x y}
val subset: x: 'a set -> y: 'a set -> {v: bool | (v = 1 and x subset y) or (v =0 and x subset y)}
val mem: x: 'a -> y: 'a set -> {v: bool | (v = 1 and mem x y) or (v = 0 and .- (mem x y))}
val card: x: 'a set -> {v: int | v = card x}
val empty: 'a -> {v: 'a set | card v = 0}
val is_empty: x: 'a set -> {v: bool | (v = 1 and card x = 0) or (v = 0 and .- (card x = 0)}
val singleton: x: 'a -> {v: 'a set | v = singleton x}
