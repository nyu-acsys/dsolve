measure nonnull = 
  | x::xs -> true 
  | []    -> (not (true))

val matrix_multiply: 
  m: {v: int| v > 0 } -> 
  n: {v: int| v > 0 } -> 
  a: {v: {v: float array | Array.length v = n} array | Array.length = m} ->
     {v: float array | Array.length v = m} 

val map_reduce: ('a -> ('b * 'c) list) -> 
		('b -> 'b -> 'b) ->
		('a list) ->
		('b * 'c) list

