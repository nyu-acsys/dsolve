qualif A(v): _meas_size t = v
qualif B(v): _meas_size v { * * } ~A
qualif C(v): _meas_size v = _meas_size ~A + 1
qualif D(v): _meas_size v = [0, 1, 2, 3, 4]

qualif E(v): v = [0, 1, 2, 3, 4]
qualif F(v): v = ~A

qualif G1(v): _meas_len v = _meas_len ~A + _meas__meas_len ~B
qualif G2(v): _meas_len v = _meas_size ~A
qualif G3(v): v = _meas_len ~A
