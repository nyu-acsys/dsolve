qualif VEQ(v)(A: bdd): var ~A { <=, < } var v
qualif VEQ(v)(A: int):     ~A { <=, < } var v
qualif OEQ(v)(A: bdd, B: bdd): ((var ~A <= var v) or (var ~B <= var v))

