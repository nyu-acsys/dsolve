type variable = {v:int | 1 <= v && v <= max_var }

type t
type view = Zero | One | Node of variable * t (*low*) * t (*high*)

val var : t -> variable
val low : t -> t
val high: t -> t
val zero: t
val one : t

val mk : variable -> low:t -> high:t -> t
val mk_var : variable -> t
val mk_not : t -> t
val mk_and : t -> t -> t
val mk_or : t -> t -> t
val mk_imp : t -> t -> t
val apply : (bool -> bool -> bool) -> t -> t -> t

val build : formula -> t
